      <rdfs:Class rdf:about="#keywords"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:ID="need">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
