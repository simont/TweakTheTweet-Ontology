    >Simon Twigger</dc:creator>
  </owl:Ontology>
  <rdfs:Class rdf:ID="trapped">
    <rdfs:comment xml:lang="en">tagged things mention people that are trapped</rdfs:comment>
    <rdfs:label xml:lang="en">Trapped</rdfs:label>
    <rdfs:subClassOf>
      <rdfs:Class rdf:ID="main_tags"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:ID="shelter">
    <rdfs:subClassOf>
      <rdfs:Class rdf:ID="keywords"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:ID="medical">
    <rdfs:comment xml:lang="en">tagged things that contain a mention of something medical</rdfs:comment>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="#keywords"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
