    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Open</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:ID="name">
    <rdfs:subClassOf rdf:resource="#data_tags"/>
  </rdfs:Class>
  <rdfs:Class rdf:ID="tagged_thing"/>
