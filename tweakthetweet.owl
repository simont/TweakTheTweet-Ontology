    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:ID="medical">
    <rdfs:comment xml:lang="en">tagged things that contain a mention of something medical</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#tagged_thing"/>
  </rdfs:Class>
  <rdfs:Class rdf:ID="volunteer">
