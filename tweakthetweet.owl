      <rdfs:Class rdf:about="#medical_personnel"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:ID="photo">
    <rdfs:subClassOf rdf:resource="#data_tags"/>
  </rdfs:Class>
  <rdfs:Class rdf:ID="number">
    <rdfs:subClassOf rdf:resource="#data_tags"/>
  </rdfs:Class>
  <rdfs:Class rdf:ID="security">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="#keywords"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:ID="rescue">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="#keywords"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:ID="source">
    <rdfs:label xml:lang="en">Source</rdfs:label>
    <rdfs:comment xml:lang="en">tagged things contain an indication of the source of the information or message they contain</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#data_tags"/>
  </rdfs:Class>
  <rdfs:Class rdf:ID="communication">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="#keywords"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:ID="tents">
    <rdfs:subClassOf rdf:resource="#shelter"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
