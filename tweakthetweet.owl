    <rdfs:label xml:lang="en">doctor</rdfs:label>
    <rdfs:domain rdf:resource="#doctor"/>
    <tn:tag_term xml:lang="en">doctor</tn:tag_term>
    <tn:tag_term xml:lang="en">docteur</tn:tag_term>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="has_transport">
    <rdfs:domain rdf:resource="#transport"/>
