    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="#main_tags">
    <rdfs:subClassOf rdf:resource="#tags"/>
  </rdfs:Class>
  <rdfs:Class rdf:ID="doctor">
