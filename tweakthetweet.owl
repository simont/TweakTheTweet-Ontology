    <rdfs:domain rdf:resource="#tweet"/>
    <rdfs:comment xml:lang="en">Official Twitter tweet identifier number</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="has_trapped">
