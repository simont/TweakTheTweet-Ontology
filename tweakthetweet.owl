      <rdfs:Class rdf:ID="personal"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:ID="offer">
    <rdfs:comment xml:lang="en">tagged thing contains an offer of some thing or service</rdfs:comment>
