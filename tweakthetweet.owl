    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:ID="help">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="#keywords"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:ID="nurse">
    <rdfs:comment xml:lang="en">Nurse or other medical staff</rdfs:comment>
    <rdfs:label xml:lang="en">nurse</rdfs:label>
    <rdfs:subClassOf>
