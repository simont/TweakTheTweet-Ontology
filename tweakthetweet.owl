    <rdfs:comment xml:lang="en">tagged thing mentions a location or place</rdfs:comment>
    <rdfs:label xml:lang="en">location</rdfs:label>
    <tn:tag_term xml:lang="fr">emplacement</tn:tag_term>
    <tn:tag_term xml:lang="en">loc</tn:tag_term>
    <tn:tag_term xml:lang="en">location</tn:tag_term>
    <rdfs:domain rdf:resource="#location"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="has_source">
